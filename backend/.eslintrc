{
  "extends": "airbnb-base",
  "rules": {
    "no-unused-vars": ["error", { "argsIgnorePattern": "next" }],
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "quotes": ["error", "double"],
    "no-console": ["error", { "allow": ["log"] }],
    "comma-dangle": ["error", {
      "arrays": "only-multiline",
      "objects": "only-multiline"
      }]
  }
}